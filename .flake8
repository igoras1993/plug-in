[flake8]
# From black compatibility config
extend-ignore = E203,E701

filename =
    *.py

exclude =
    # No need to traverse our git directory
    .git,
    # There's no value in checking cache directories
    __pycache__,
    # Mostly autogenerated
    src/alembic/versions/,
    .ci,


# Score thing.
max-complexity = 10

# From black compatibility config
max-line-length = 90
